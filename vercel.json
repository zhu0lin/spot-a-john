{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/update-database",
      "dest": "/src/api/update-database.js"
    }
  ],
  "crons": [
    {
      "path": "/api/update-database",
      "schedule": "0 9 1 * *"
    }
  ]
}